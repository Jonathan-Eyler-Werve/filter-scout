{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.2.0.min.js", "content.js"]
    }
  ],

  "background": {
    "scripts": ["background.js"]
  }
}